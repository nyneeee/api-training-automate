{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "status": {
      "type": "string"
    },
    "statusDesc": {
      "type": "string"
    },
    "total": {
      "type": "number"
    },
    "data": {
      "type": "array",
      "items":
        {
          "type": "object",
          "properties": {
            "commercialName": {
              "type": "string"
            },
            "company": {
              "type": "string"
            },
            "matCode": {
              "type": "string"
            },
            "brand": {
              "type": ["string","null"]
            },
            "model": {
              "type": ["string","null"]
            },
            "color": {
              "type": ["string","null"]
            },
            "productType": {
              "type": "string"
            },
            "productSubtype": {
              "type": ["string","null"]
            },
            "thumbnail": {
              "type": ["string","null"]
            },
            "prices": {
              "type": "array",
              "items":
                {
                  "type": "object",
                  "properties": {
                    "priceGroup": {
                      "type": "string"
                    },
                    "priceExcludeVat": {
                      "type": "number"
                    },
                    "priceIncludeVat": {
                      "type": "number"
                    },
                    "priceVatAmount": {
                      "type": "number"
                    },
                    "startDate": {
                      "type": "string"
                    },
                    "endDate": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "priceGroup",
                    "priceExcludeVat",
                    "priceIncludeVat",
                    "startDate"
                  ]
                }
            }
          },
          "required": [
            "commercialName",
            "company",
            "matCode",
            "productType",
            "prices"
          ]
        }
    }
  },
  "required": [
    "status",
    "statusDesc",
    "total",
    "data"
  ]
}