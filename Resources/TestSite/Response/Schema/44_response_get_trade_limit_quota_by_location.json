{
      "$schema": "http://json-schema.org/draft-04/schema#",
      "type": "object",
      "properties": {
        "statusCode": {
          "type": "string"
        },
        "statusDesc": {
          "type": "string"
        },
        "data": {
          "type": "array",
          "items": 
            {
              "type": "object",
              "properties": {
                "groupCode": {
                  "type": "string"
                },
                "groupName": {
                  "type": "string"
                },
                "effectiveStartDate": {
                  "type": "string"
                },
                "effectiveEndDate": {
                  "type": "string"
                },
                "criterias": {
                  "type": "array",
                  "items": 
                    {
                      "type": "object",
                      "properties": {
                        "tradeNo": {
                          "type": "string"
                        }
                      },
                      "required": [
                        "tradeNo"
                      ]
                    }
                },
                "locationCode": {
                  "type": "string"
                },
                "limitQuota": {
                  "type": "number"
                }
              },
              "required": [
                "groupCode",
                "groupName",
                "effectiveStartDate",
                "criterias",
                "locationCode",
                "limitQuota"
              ]
            }
        }
      },
      "required": [
        "statusCode",
        "statusDesc",
        "data"
      ]
}