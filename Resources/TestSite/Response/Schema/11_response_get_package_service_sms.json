{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "statusCode": {
        "type": "string"
      },
      "statusDesc": {
        "type": "string"
      },
      "data": {
        "type": "array",
        "items":
          {
            "type": "object",
            "properties": {
              "allocate": {
                "type": "array"
              },
              "durationFrom": {
                "type": "number"
              },
              "durationTo": {
                "type": "number"
              },
              "durationUnit": {
                "type": "string"
              },
              "billCycleValue":{
                "type":"number"
              },
              "billCycleUnit":{
                "type":"string"
              },
              "freeResource": {
                "type": "number"
              },
              "freeResourceUnit": {
                "type": "string"
              },
              "messageRating": {
                "type": "array",
                "items": 
                  {
                    "type": "object",
                    "properties": {
                      "rateProductType": {
                        "type": "string"
                      },
                      "exceedRateExcVat": {
                        "type": "number"
                      },
                      "exceedRateIncVat": {
                        "type": "number"
                      }
                    }
                  }
              }
            },
            "required": [
              "durationFrom",
              "durationTo",
              "durationUnit"
            ]
          }
      }
    },
    "required": [
      "statusCode",
      "statusDesc",
      "data"
    ]
  }