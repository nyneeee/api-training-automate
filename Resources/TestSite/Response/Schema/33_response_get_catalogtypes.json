{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "statusCode": {
        "type": "string"
      },
      "statusDesc": {
        "type": "string"
      },
      "offset": {
        "type": "number"
      },
      "max": {
        "type": "number"
      },
      "catalogTypes": {
        "type": "array",
        "items":
          {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "icon": {
                "type": ["string", "null"]
              }
            },
            "required": [
              "name"
            ]
          }
      }
    },
    "required": [
      "statusCode",
      "statusDesc",
      "offset",
      "max",
      "catalogTypes"
    ]
  }