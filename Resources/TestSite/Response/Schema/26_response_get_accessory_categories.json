    },
    "statusDesc": {
      "type": "string"
    },
    "total": {
      "type": "number"
    },
    "data": {
      "type": "array",
      "items": {
          "type": "object",
          "properties": 
          {
            "masterId": {
              "type": "number"
            },
            "category": {
              "type": "string"
            },
            "subCategory": {
