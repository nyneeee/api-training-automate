{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "statusCode": {
        "type": "string"
      },
      "statusDesc": {
        "type": "string"
      },
      "offset": {
        "type": "number"
      },
      "max": {
        "type": "number"
      },
      "products": {
        "type": "array",
        "items":
          {
            "type": "object",
            "properties": {
              "commercialName": {
                "type": "string"
              },
              "brand": {
                "type": "string"
              },
              "model": {
                "type": "string"
              },
              "thumbnail": {
                "type": ["string","null"]
              },
              "materialCodes": {
                "type": "array",
                "items":
                  {
                    "type": "string"
                  }
              },
              "catalogProducts": {
                "type": "array",
                "items":
                  {
                    "type": "object",
                    "minProperties": 1,
                    "properties": {
                      "catalogType": {
                        "type": "string"
                      },
                      "catalogSubType": {
                        "type": "string"
                      },
                      "category": {
                        "type": "string"
                      }
                    }
                  }
              },
              "normalPrice": {
                "type": ["number","null"]
              },
              "promotionPrice": {
                "type": "object",
                "properties": {
                  "min": {
                    "type": ["number","null"]
                  },
                  "max": {
                    "type": ["number","null"]
                  }
                },
                "required": [
                  "min",
                  "max"
                ]
              }
            },
            "required": [
              "commercialName",
              "brand",
              "model",
              "normalPrice",
              "promotionPrice"
            ]
          }
      }
    },
    "required": [
      "statusCode",
      "statusDesc",
      "offset",
      "max",
      "products"
    ]
  }