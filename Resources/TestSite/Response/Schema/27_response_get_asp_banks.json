{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "statusCode": {
        "type": "string"
      },
      "statusDesc": {
        "type": "string"
      },
      "banks": {
        "type": "array",
        "items": 
          {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "abb": {
                "type": "string"
              },
              "imageUrl": {
                "type": ["string" , "null"]
              }
            },
            "required": [
              "name",
              "abb",
              "imageUrl"
            ]
          }
      }
    },
    "required": [
      "statusCode",
      "statusDesc"
    ]
  }