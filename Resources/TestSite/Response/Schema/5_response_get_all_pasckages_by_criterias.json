    },
    "max": {
      "type": ["number","string"]
    },
    "data": {
      "type": "array",
      "items": 
        {
          "type": "object",
          "properties": {
            "id": {
              "type": "number"
            },
            "title": {
              "type": "string"
            },
            "detailTH": {
              "type": ["string","null"]
            },
            "detailEN": {
              "type": ["string","null"]
            },
            "thumbnail": {
              "type": ["string","null"]
            },
            "feedItemId": {
              "type": "string"
            },
            "promotionCode": {
              "type": ["string","null"]
            },
            "offeringCode": {
              "type": ["string","null"]
            },
            "offeringId": {
              "type": ["string","null"]
            },
            "packageId": {
              "type": ["string","null"]
            },
            "featureCode": {
              "type": ["string","null"]
            },
            "publish": {
              "type": "boolean"
            },
            "priority": {
              "type": "number"
            },
            "lastUpdated": {
              "type": "number"
            },
            "prototype": {
              "type": "string"
            },
            "customAttributes": {
              "type": "object"
            },
            "internalAttributes": {
              "type": "object"
            },
            "tags":{
              "type": "array",
