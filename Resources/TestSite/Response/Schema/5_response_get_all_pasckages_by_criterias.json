                    "billingSystem": {
                      "type": "string"
                    },
                    "productCode": {
                      "type": "string"
                    },
                    "productName": {
                      "type": "string"
                    },
                    "priceExclVat": {
                      "type": "number"
                    },
                    "priceInclVat": {
                      "type": "number"
                    },
                    "productType": {
                      "type": "string"
                    },
                    "wordInStatementThai": {
                      "type": "string"
                    },
                    "wordInStatementEng": {
                      "type": "string"
                    }
                  }
                }
            },
            "accountFees": {
              "type": "array",
              "items": 
                {
                  "type": "object",
                  "properties": {
                    "billingSystem": {
                      "type": "string"
                    },
                    "productCode": {
                      "type": "string"
                    },
                    "productName": {
                      "type": "string"
                    },
                    "priceExclVat": {
                      "type": "number"
                    },
                    "priceInclVat": {
                      "type": "number"
                    },
                    "productType": {
                      "type": "string"
                    },
                    "wordInStatementThai": {
                      "type": "string"
                    },
                    "wordInStatementEng": {
                      "type": "string"
                    }
                  }
                }
            }
          },
          "required": [
            "id",
            "title",
            "detailTH",
            "detailEN",
            "thumbnail",
            "feedItemId",
            "publish",
            "priority",
            "lastUpdated",
            "prototype"
          ]
        }
    },
    "total": {
      "type": "string"
    }
  },
  "required": [
    "status",
    "statusDesc",
    "offset",
    "max",
    "data"
  ]
