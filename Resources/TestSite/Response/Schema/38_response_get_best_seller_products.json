{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "statusCode": {
        "type": "string"
      },
      "statusDesc": {
        "type": "string"
      },
      "offset": {
        "type": "number"
      },
      "rowCount": {
        "type": "number"
      },
      "products": {
        "type": "array",
        "items": 
          {
            "type": "object",
            "properties": {
              "brand": {
                "type": "string"
              },
              "name": {
                "type": "string"
              },
              "model": {
                "type": "string"
              },
              "color": {
                "type": ["string","null"]
              },
              "imageUrl": {
                "type": "string"
              },
              "priority": {
                "type": "number"
              },
              "itemType": {
                "type": "string"
              }
            },
            "required": [
              "brand",
              "name",
              "model",
              "priority",
              "itemType"
            ]
          }
      }
    },
    "required": [
      "statusCode",
      "statusDesc",
      "offset",
      "rowCount",
      "products"
    ]
  }