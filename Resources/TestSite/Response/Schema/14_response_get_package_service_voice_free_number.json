{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "statusCode": {
      "type": "string"
    },
    "statusDesc": {
      "type": "string"
    },
    "data": {
      "type": "array",
      "items": 
        {
          "type": "object",
          "properties": {
            "freeResourceType": {
              "type": "string"
            },
            "durationType": {
              "type": "string"
            },
            "durationFrom": {
              "type": "string"
            },
            "durationTo": {
              "type": "string"
            },
            "durationUnit": {
              "type": "string"
            },
            "billCycleType": {
              "type": "string"
            },
            "billCycleValue": {
              "type": ["null","string"]
            },
            "billCycleUnit": {
              "type": ["null","string"]
            },
            "maxFnNumber": {
              "type": "string"
            },
            "fnNetwork": {
              "type": "string"
            },
            "fnType": {
              "type": "string"
            },
            "fnRateExcVat": {
              "type": "string"
            },
            "fnRateExcVatUnit": {
              "type": "string"
            },
            "fnRateIncVat": {
              "type": "string"
            },
            "fnRateIncVatUnit": {
              "type": "string"
            },
            "fnFreeCall": {
              "type": ["null","string"]
            },
            "fnFreeCallUnit": {
              "type": ["null","string"]
            },
            "priceTypePattern": {
              "type": "string"
            },
            "dayOfWeek": {
              "type": "string"
            },
            "fromTiming": {
              "type": "string"
            },
            "toTiming": {
              "type": "string"
            },
            "peakTimeFlag": {
              "type": "null"
            },
            "deductType": {
              "type": "null"
            },
            "channels": {
              "type": "array",
              "items": 
                {
                  "type": "object",
                  "properties": {
                    "channelName": {
                      "type": "string"
                    },
                    "actionFn": {
                      "type": "array",
                      "items": 
                        {
                          "type": "object",
                          "properties": {
                            "action": {
                              "type": ["string","null"]
                            },
                            "feeExcVat": {
                              "type": ["string","null"]
                            },
                            "feeIncVat": {
                              "type": ["string","null"]
                            }
                          }
                        }
                    }
                  },
                  "required": [
                    "channelName",
                    "actionFn"
                  ]
                }
            }
          },
          "required": [
            "freeResourceType",
            "durationType",
            "durationFrom",
            "durationTo",
            "durationUnit"
          ]
        }
    }
  },
  "required": [
    "statusCode",
    "data"
  ]
}