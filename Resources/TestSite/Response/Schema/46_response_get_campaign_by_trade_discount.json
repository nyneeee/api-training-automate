{
      "$schema": "http://json-schema.org/draft-04/schema#",
      "type": "object",
      "properties": {
        "statusCode": {
          "type": "string"
        },
        "status": {
          "type": "string"
        },
        "statusDesc": {
          "type": "string"
        },
        "data": {
          "type": "array",
          "items": 
            {
              "type": "object",
              "properties": {
                "tradeProductId": {
                  "type": "number"
                },
                "startDate": {
                  "type": "string"
                },
                "endDate": {
                  "type": "string"
                },
                "location": {
                  "type": ["number","null"]
                },
                "trades": {
                  "type": "array",
                  "items": 
                    {
                      "type": "object",
                      "properties": {
                        "tradeNo": {
                          "type": ["string","null"]
                        },
                        "campaignName": {
                          "type": ["string","null"]
                        },
                        "campaignCode": {
                          "type": ["string","null"]
                        },
                        "durationContract": {
                          "type": ["string","null"]
                        },
                        "discount": {
                          "type": "object",
                          "properties": {
                            "tradeDiscountId": {
                              "type": "number"
                            },
                            "payAdvanceGroupId": {
                              "type": ["number","null"]
                            },
                            "tradePriceExcludeVat": {
                              "type": ["number","null"]
                            },
                            "tradePriceIncludeVat": {
                              "type": ["number","null"]
                            },
                            "tradePriceVat": {
                              "type": ["number","null"]
                            },
                            "discountExcludeBy": {
                              "type": ["string","null"]
                            },
                            "discountExcludeVat": {
                              "type": ["number","null"]
                            },
                            "discountIncludeBy": {
                              "type": ["string","null"]
                            },
                            "discountSpecialIncludeVat": {
                              "type": ["number","null"]
                            },
                            "vatRate": {
                              "type": ["string","null"]
                            },
                            "startDate": {
                              "type": "string"
                            },
                            "endDate": {
                              "type": "string"
                            }
                          },
                          "required": [
                            "tradeDiscountId"
                          ]
                        },
                        "payAdvance": {
                          "type": "object",
                          "properties": {
                            "tradeAirtimeId": {
                              "type": ["number","null"]
                            },
                            "priceIncludeVat": {
                              "type": ["number","null"]
                            },
                            "payAdvanceGroupId": {
                              "type": ["number","null"]
                            }
                          },
                          "required": [
                            "tradeAirtimeId",
                            "priceIncludeVat"
                          ]
                        }
                      },
                      "required": [
                        "tradeNo",
                        "campaignName",
                        "campaignCode",
                        "discount"
                      ]
                    }
                },
                "products": {
                  "type": "array",
                  "items": 
                    {
                      "type": "object",
                      "properties": {
                        "commercialName": {
                          "type": ["string","null"]
                        },
                        "company": {
                          "type": ["string","null"]
                        },
                        "brand": {
                          "type": ["string","null"]
                        },
                        "model": {
                          "type": ["string","null"]
                        },
                        "color": {
                          "type": ["string","null"]
                        },
                        "matCode": {
                          "type": "string"
                        },
                        "productType": {
                          "type": ["string","null"]
                        },
                        "productSubType": {
                          "type": ["string","null"]
                        },
                        "images": {
                          "type": ["string","null"]
                        },
                        "prices": {
                          "type": "array",
                          "items": 
                            {
                              "type": "object",
                              "properties": {
                                "priceGroup": {
                                  "type": ["string","null"]
                                },
                                "priceExcludeVat": {
                                  "type": ["number","null"]
                                },
                                "priceIncludeVat": {
                                  "type": ["number","null"]
                                },
                                "priceVatAmount": {
                                  "type": ["number","null"]
                                },
                                "vatRate": {
                                  "type": ["string","null"]
                                },
                                "startDate": {
                                  "type": ["string","null"]
                                },
                                "endDate": {
                                  "type": ["string","null"]
                                }
                              },
                              "required": [
                                "priceGroup",
                                "priceExcludeVat",
                                "priceIncludeVat",
                                "priceVatAmount",
                                "vatRate",
                                "startDate"
                              ]
                            }
                        }
                      },
                      "required": [
                        "company",
                        "brand",
                        "model",
                        "matCode",
                        "productType",
                        "productSubType",
                        "prices"
                      ]
                    }
                },
                "saleChannels": {
                  "type": "array",
                  "items": 
                    {
                      "type": "string"
                    }
                }
              },
              "required": [
                "tradeProductId",
                "startDate",
                "location",
                "trades",
                "products",
                "saleChannels"
              ]
            }
        }
      },
      "required": [
        "statusCode",
        "statusDesc",
        "data"
      ]
}