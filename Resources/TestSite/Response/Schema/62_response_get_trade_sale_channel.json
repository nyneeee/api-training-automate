{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
	  "statusCode": {
		"type": "string"
	  },
	  "statusDesc": {
		"type": "string"
	  },
	  "tradeSaleChannels": {
		"type": "array",
		"minItems": 1,
		"items": 
		  {
			"type": "object",
			"properties": {
			  "tradeNo": {
				"type": "string"
			  },
			  "saleChannels": {
				"type": "array",
				"items": 
				  {
					"type": "object",
					"properties": {
					  "region": {
						"type": ["string" , "null"]
					  },
					  "saleChannel": {
						"type": "string"
					  }
					},
					"required": [
					  "saleChannel"
					]
				  }
			  }
			},
			"required": [
			  "tradeNo"
			]
		  }
	  }
	},
	"required": [
	  "statusCode",
	  "statusDesc",
	  "tradeSaleChannels"
	]
  }