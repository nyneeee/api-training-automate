{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "statusCode": {
      "type": "string"
    },
    "statusDesc": {
      "type": "string"
    },
    "data": {
      "type": "array",
      "items":
        {
          "type": "object",
          "properties": {
            "id": {
              "type": "number"
            },
            "title": {
              "type": "string"
            },
            "detailTH": {
              "type": ["string","null"]
            },
            "detailEN": {
              "type": ["string","null"]
            },
            "thumbnail": {
              "type": ["string","null"]
            },
            "feedItemId": {
              "type": "string"
            },
            "featureCode": {
              "type": ["string","null"]
            },
            "publish": {
              "type": "boolean"
            },
            "prototype": {
              "type": ["string","null"]
            },
            "lastUpdated":{
              "type": "string"
            },
            "customAttributes": {
              "type": "object"
            }
          },
          "required": [
            "id",
            "title",
            "detailTH",
            "detailEN",
            "thumbnail",
            "feedItemId",
            "featureCode",
            "publish",
            "prototype",
            "lastUpdated"
          ]
        }
    }
  },
  "required": [
    "statusCode",
    "statusDesc",
    "data"
  ]
}