{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "statusCode": {
            "type": "string"
        },
        "statusDesc": {
            "type": "string"
        },
        "offset": {
            "type": "number"
        },
        "max": {
            "type": "number"
        },
        "total": {
            "type": "string"
        },
        "priceOptions": {
            "type": "array",
            "items": 
                {
                    "type": "object",
                    "properties": {
                        "campaign": {
                            "type": "object",
                            "properties": {
                                "campaignId": {
                                    "type": ["number", "null"]
                                },
                                "campaignName": {
                                    "type": ["string" , "null"]
                                },
                                "conditionCode": {
                                    "type": ["string" , "null"]
                                },
                                "packageKeyRef": {
                                    "type": ["string" , "null"]
                                },
                                "minPackagePrice": {
                                    "type": ["number" , "null"]
                                },
                                "maxPackagePrice": {
                                    "type": ["number" , "null"]
                                }
                            }
                        },
                        "promotionPrice": {
                            "type": ["number", "null"]
                        },
                        "criterias": {
                            "type": ["array" , "null"],
                            "items": {
                                "type": "object",
                                "minProperties": 1
                            }
                        },
                        "discount": {
                            "type": ["object", "null"],
                            "properties": {
                                "tradeDiscountId": {
                                    "type": "number"
                                },
                                "tradePriceExcludeVat": {
                                    "type": ["number", "null"]
                                },
                                "tradePriceIncludeVat": {
                                    "type": ["number", "null"]
                                },
                                "tradePriceVatAmount": {
                                    "type": ["number", "null"]
                                },
                                "discountExcludeBy": {
                                    "type": ["string", "null"]
                                },
                                "discountExcludeVat": {
                                    "type": ["number", "null"]
                                },
                                "specialIncludeVat": {
                                    "type": ["number", "null"]
                                },
                                "specialIncludeBy": {
                                    "type": ["string", "null"]
                                },
                                "requireChangePack": {
                                    "type": "boolean"
                                },
                                "option": {
                                    "type": ["string", "null"]
                                },
                                "payAdvanceGroupId": {
                                    "type": ["number", "null"]
                                },
                                "tradePrivilegeId": {
                                    "type": ["number", "null"]
                                },
                                "limitContract": {
                                    "type": ["number", "null"]
                                }
                            }
                        },
                        "privilege": {
                            "type": "object",
                            "properties": {
                                "tradePrivilegeId": {
                                    "type": ["number", "null"]
                                },
                                "privilegeId": {
                                    "type": ["string", "null"]
                                },
                                "ussdCode": {
                                    "type": ["string", "null"]
                                }
                            },
                            "required": [
                                "tradePrivilegeId",
                                "privilegeId"
                            ]
                        },
                        "payAdvance": {
                            "type": "object",
                            "properties": {
                                "payAdvanceGroupId": {
                                    "type": ["number", "null"]
                                }
                            },
                            "required": [
                                "payAdvanceGroupId"
                            ]
                        },
                        "tradeNo": {
                            "type": ["string" , "null"]
                        },
                        "tradeProductId": {
                            "type": ["number"]
                        },
                        "durationContract": {
                            "type": ["string", "null"]
                        },
                        "contractId": {
                            "type": ["number", "null"]
                        },
                        "maxReceiveFreegoods": {
                            "type": ["number", "null"]
                        }
                    },
                    "required": [
                        "campaign",
                        "tradeNo",
                        "tradeProductId"
                    ]
                }
        }
    },
    "required": [
        "statusCode",
        "statusDesc",
        "offset",
        "max",
        "total",
        "priceOptions"
    ]
}