{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
	  "statusCode": {
		"type": "string"
	  },
	  "statusDesc": {
		"type": "string"
	  },
	  "products": {
		"type": "array",
		"items": {
		  "type": "object",
		  "properties": {
			"name": {
			  "type": "string",
			  "enum": [
				"screenSize",
				"memory",
				"mainCamera",
				"secondaryCamera",
				"cpuType",
				"cpuCore",
				"cpuSpeed",
				"operatingSystem",
				"operatingSystemVersion",
				"SIMSlot",
				"screenResolution",
				"video",
				"specCard"
			  ]
			},
			"value": {
			  "type": "string"
			}
		  },
		  "required": [
			"name", 
			"value"
		]
		}
	  }
	},
	"required": [
		"statusCode", 
		"statusDesc",
		"products"
	],
    "additionalProperties": false
}