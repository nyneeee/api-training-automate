{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "status": {
      "type": "string"
    },
    "statusDesc": {
      "type": "string"
    },
    "offset": {
      "type": "number"
    },
    "maxRow": {
      "type": "number"
    },
    "total": {
      "type": "number"
    },
    "products": {
      "type": "array",
      "items":
        {
          "type": "object",
          "properties": {
            "company": {
              "type": "string"
            },
            "brand": {
              "type": "string"
            },
            "commercialName":{
              "type": "string"
            },
            "model": {
              "type": "string"
            },
            "color": {
              "type": "string"
            },
            "matCode": {
              "type": "string"
            },
            "productType": {
              "type": "string"
            },
            "productSubtype": {
              "type": "string"
            },
            "prices": {
              "type": "array",
              "items":
                {
                  "type": "object",
                  "properties": {
                    "priceType": {
                      "type": "string"
                    },
                    "priceIncludeVat": {
                      "type": "number"
                    },
                    "priceExcludeVat": {
                      "type": "number"
                    },
                    "vatRate": {
                      "type": "string"
                    },
                    "vatAmount": {
                      "type": "string"
                    },
                    "effectiveDate": {
                      "type": "string"
                    },
                    "expireDate": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "priceType",
                    "priceIncludeVat",
                    "priceExcludeVat",
                    "effectiveDate"
                  ]
                }
            },
            "campaigns": {
              "type": "array",
              "items":
                {
                  "type": "object",
                  "properties": {
                    "campaignName": {
                      "type": "string"
                    },
                    "campaignCode": {
                      "type": "string"
                    },
                    "conditionCode": {
                      "type": "string"
                    },
                    "requireCheckBlacklistAcrossOper": {
                      "type": ["string"],
                      "enum": ["Y", "N"]
                    }
                  },
                  "required": [
                    "campaignName",
                    "campaignCode",
                    "requireCheckBlacklistAcrossOper"
                  ]
                }
            },
            "trades": {
              "type": "array",
              "items":
                {
                  "type": "object",
                  "properties": {
                    "tradeNo": {
                      "type": "string"
                    },
                    "tradeName": {
                      "type": "string"
                    },
                    "campaignCode": {
                      "type": "string"
                    },
                    "campaignName": {
                      "type": "string"
                    },
                    "startDate": {
                      "type": "string"
                    },
                    "endDate": {
                      "type": "string"
                    },
                    "saleType": {
                      "type": "string"
                    },
                    "prebookingType": {
                      "type": "string"
                    },
                    "ussdCode": {
                      "type": "string"
                    },
                    "serviceLockHs": {
                      "type": "string"
                    },
                    "simLock": {
                      "type": ["string","null"]
                    },
                    "contractId": {
                      "type": "number"
                    },
                    "payAdvances": {
                      "type": "array",
                      "items": 
                        {
                          "type": "object",
                          "properties": {
                            "tradeAirtimeId": {
                              "type": "number"
                            },
                            "materialAirtime": {
                              "type": "string"
                            },
                            "materialAirtimeDesc": {
                              "type": "string"
                            },
                            "payadvanceGroupId": {
                              "type": "number"
                            }
                          },
                          "required": [
                            "tradeAirtimeId",
                            "materialAirtime",
                            "materialAirtimeDesc"
                          ]
                        }
                    },
                    "discounts": {
                      "type": "array",
                      "items":
                        {
                          "type": "object",
                          "properties": {
                            "tradeDiscountId": {
                              "type": "number"
                            },
                            "priceType": {
                              "type": "string"
                            },
                            "discountExcludeBy": {
                              "type": "string"
                            },
                            "discountIncludeBy": {
                              "type": "string"
                            },
                            "depositExcludeVat": {
                              "type": "number"
                            },
                            "depositIncludeVat": {
                              "type": "number"
                            },
                            "depositVatAmount": {
                              "type": "number"
                            },
                            "discountExcludeVat": {
                              "type": "number"
                            },
                            "discountIncludeVat": {
                              "type": "number"
                            },
                            "tradePriceExcludeVat": {
                              "type": "number"
                            },
                            "tradePriceIncludeVat": {
                              "type": "number"
                            },
                            "tradePriceAmount": {
                              "type": "number"
                            },
                            "vatRate": {
                              "type": "number"
                            },
                            "durationContract": {
                              "type": "string"
                            },
                            "option": {
                              "type": "string"
                            },
                            "startDate": {
                              "type": "string"
                            },
                            "endDate": {
                              "type": "string"
                            },
                            "matAirtimeId": {
                              "type": "number"
                            },
                            "payAdvanceGroupId": {
                              "type": "number"
                            },
                            "privilegeId": {
                              "type": "number"
                            },
                            "tradePrivilegeId": {
                              "type": "number"
                            },
                            "minimumPackagePrice": {
                              "type": "number"
                            },
                            "minimumPackageOntopPrice": {
                              "type": "number"
                            },
                            "maximumPackagePrice": {
                              "type": "number"
                            },
                            "maximumPackageOnTopPrice": {
                              "type": "number"
                            },
                            "packageKeyRef": {
                              "type": "string"
                            },
                            "packageOnTopKeyRef": {
                              "type": "string"
                            },
                            "requireChangePack": {
                              "type": "boolean"
                            }
                          },
                          "required": [
                            "tradeDiscountId",
                            "priceType"
                          ]
                        }
                    },
                    "payments": {
                      "type": "array",
                      "items":
                        {
                          "type": "object",
                          "properties": {
                            "cardType": {
                              "type": "string"
                            },
                            "method": {
                              "type": "string"
                            },
                            "banks": {
                              "type": "array",
                              "items":
                                {
                                  "type": "object",
                                  "minProperties": 1,
                                  "properties": {
                                    "bankDescEn": {
                                      "type": "string"
                                    },
                                    "bankDescTh": {
                                      "type": "string"
                                    },
                                    "installments": {
                                      "type": "array",
                                      "items":
                                        {
                                          "type": "object",
                                          "properties": {
                                            "installId": {
                                              "type": "number"
                                            },
                                            "installmentAbb": {
                                              "type": "string"
                                            }
                                          },
                                          "required": [
                                            "installId",
                                            "installmentAbb"
                                          ]
                                        }
                                    }
                                  }
                                }
                            }
                          },
                          "required": [
                            "method"
                          ]
                        }
                    },
                    "freegoods": {
                      "type": "array",
                      "items": 
                        {
                          "type": "object",
                          "properties": {
                            "tradeFreegoodsId": {
                              "type": "number"
                            },
                            "materialCode": {
                              "type": "string"
                            },
                            "description": {
                              "type": "string"
                            },
                            "quantity": {
                              "type": "number"
                            }
                          },
                          "required": [
                            "tradeFreegoodsId",
                            "materialCode"
                          ]
                        }
                    },
                    "privileges": {
                      "type": "array",
                      "items":
                        {
                          "type": "object",
                          "properties": {
                            "tradePrivilegeId": {
                              "type": "number"
                            },
                            "ussdCode": {
                              "type": "string"
                            }
                          },
                          "required": [
                            "tradePrivilegeId"
                          ]
                        }
                    },
                    "criterias": {
                      "type": "array",
                      "items": {
                          "type": "object",
                          "minProperties": 1
                      }
                    },
                    "channels": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "minProperties": 1
                      }
                    },
                    "requireCheckQuota": {
                      "type": ["string"],
                      "enum": ["Y", "N"]
                    }
                  },
                  "required": [
                    "tradeNo",
                    "campaignCode",
                    "saleType",
                    "prebookingType",
                    "ussdCode"
                  ]
                }
            }
          },
          "required": [
            "company",
            "model",
            "color",
            "matCode",
            "productType",
            "prices",
            "campaigns",
            "trades"
          ]
        }
    }
  },
  "required": [
    "status",
    "statusDesc",
    "products",
    "offset",
    "maxRow",
    "total"
  ]
}