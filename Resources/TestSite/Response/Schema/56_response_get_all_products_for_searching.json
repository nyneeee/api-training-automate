{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "statusCode": {
        "type": "string"
      },
      "statusDesc": {
        "type": "string"
      },
      "total": {
        "type": "number"
      },
      "offset": {
        "type": "number"
      },
      "maxRow": {
        "type": "number"
      },
      "data": {
        "type": "array",
        "items": 
          {
            "type": "object",
            "properties": {
              "company": {
                "type": "string"
              },
              "brand": {
                "type": ["string" , "null"]
              },
              "model": {
                "type": ["string" , "null"]
              },
              "color": {
                "type": ["string" , "null"]
              },
              "matCode": {
                "type": "string"
              },
              "matType": {
                "type": "string"
              },
              "commercialName": {
                "type": ["string" , "null"]
              },
              "colorCode": {
                "type": ["string" , "null"]
              },
              "groupName": {
                "type": ["string" , "null"]
              },
              "productType": {
                "type": "string"
              },
              "productSubtype": {
                "type": ["string" , "null"]
              },
              "effectiveDate": {
                "type": "string"
              },
              "lastUpdateDate": {
                "type": ["string" , "null"]
              },
              "priceInclVat": {
                "type": "number"
              },
              "thumbnail": {
                "type": ["string" , "null"]
              },
              "images": {
                "type": "array",
                "items": 
                  {
                    "imageUrl": {
                      "type": ["string" , "null"]
                    }
                  }
              }
            },
            "required": [
              "company",
              "matCode",
              "matType"
            ]
          }
      }
    },
    "required": [
      "statusCode",
      "statusDesc",
      "total",
      "offset",
      "maxRow",
      "data"
    ]
  }